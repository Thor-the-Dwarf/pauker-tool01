  "questions": [
    {
      "id": "q01",
      "hotkey": "1",
      "text": "Arbeitet es nach festen Regeln und ist die Ausgabe bei gleicher Eingabe immer gleich?",
      "attributeKey": "deterministic_rules"
    },
    {
      "id": "q02",
      "hotkey": "2",
      "text": "Lernt es Muster aus Daten (statt fest programmierter Regeln)?",
      "attributeKey": "learns_from_data"
    },
    {
      "id": "q03",
      "hotkey": "3",
      "text": "Basiert es auf Deep Learning (viele neuronale Schichten)?",
      "attributeKey": "deep_learning_based"
    },
    {
      "id": "q04",
      "hotkey": "4",
      "text": "Erzeugt es neue Inhalte (Text/Bilder/Code) statt nur zu klassifizieren oder zu prognostizieren?",
      "attributeKey": "generates_new_content"
    },
    {
      "id": "q05",
      "hotkey": "5",
      "text": "Liegt der Fokus auf Text- oder Code-Erzeugung?",
      "attributeKey": "text_code_generation_focus"
    },
    {
      "id": "q06",
      "hotkey": "6",
      "text": "Liegt der Fokus auf Bild-Erzeugung?",
      "attributeKey": "image_generation_focus"
    },
    {
      "id": "q07",
      "hotkey": "7",
      "text": "Ist es der Lernprozess, bei dem ein Modell aus Daten 'trainiert' wird?",
      "attributeKey": "process_training"
    },
    {
      "id": "q08",
      "hotkey": "8",
      "text": "Ist es die Nutzungsphase, in der ein trainiertes Modell eine Antwort erzeugt?",
      "attributeKey": "process_inference"
    },
    {
      "id": "q09",
      "hotkey": "9",
      "text": "Ist es die Eingabe/Anweisung, mit der man KI steuert?",
      "attributeKey": "is_prompt"
    },
    {
      "id": "q10",
      "hotkey": "10",
      "text": "Ist es die Begrenzung, wie viel Text ein Modell gleichzeitig berücksichtigen kann?",
      "attributeKey": "is_context_window"
    },
    {
      "id": "q11",
      "hotkey": "11",
      "text": "Ist es das Risiko, dass KI plausible, aber erfundene/falsche Aussagen erzeugt?",
      "attributeKey": "risk_hallucination"
    },
    {
      "id": "q12",
      "hotkey": "12",
      "text": "Ist es das Risiko verzerrter oder diskriminierender Ergebnisse durch Trainingsdaten?",
      "attributeKey": "risk_bias"
    },
    {
      "id": "q13",
      "hotkey": "13",
      "text": "Geht es um personenbezogene Daten/Geheimnisse und was in Eingaben nicht stehen darf?",
      "attributeKey": "risk_privacy"
    },
    {
      "id": "q14",
      "hotkey": "14",
      "text": "Geht es um Rechte an Inhalten (Training/Output kann urheberrechtlich problematisch sein)?",
      "attributeKey": "risk_copyright"
    },
    {
      "id": "q15",
      "hotkey": "15",
      "text": "Geht es um Angriffe über Anweisungen/Eingaben, die Regeln aushebeln oder Daten abgreifen sollen?",
      "attributeKey": "risk_prompt_injection"
    },
    {
      "id": "q16",
      "hotkey": "16",
      "text": "Ist es die EU-Regulierung, die KI nach Risiko-Klassen einordnet und Pflichten definiert?",
      "attributeKey": "is_regulation_ai_act"
    },
    {
      "id": "q17",
      "hotkey": "17",
      "text": "Ist es die Risikokategorie mit besonders strengen Anforderungen (z. B. HR/Kredit/Medizin)?",
      "attributeKey": "is_high_risk_category"
    },
    {
      "id": "q18",
      "hotkey": "18",
      "text": "Ist es die Maßnahme, dass ein Mensch Ergebnisse kontrolliert und Entscheidungen nicht blind automatisiert werden?",
      "attributeKey": "is_human_oversight_measure"
    }
  ]
}
