    { "id": "q09", "hotkey": "9", "text": "Hat es direkt mit Medien/Pixeln/Farbtiefe zu tun?", "attributeKey": "relates_to_media_pixels" },
    { "id": "q10", "hotkey": "10", "text": "Hat es direkt mit Zeit/Dauer/Laufzeit zu tun?", "attributeKey": "relates_to_time_or_duration" },
