  "questions": [
    {
      "id": "q01",
      "hotkey": "1",
      "text": "Ist es ein agiles Vorgehensmodell oder Framework?",
      "attributeKey": "agile_framework"
    },
    {
      "id": "q02",
      "hotkey": "2",
      "text": "Arbeitet es mit festen Sprints bzw. Timeboxes?",
      "attributeKey": "uses_sprints_timeboxes"
    },
    {
      "id": "q03",
      "hotkey": "3",
      "text": "Gibt es klar definierte Rollen (z. B. Product Owner, Scrum Master, Team)?",
      "attributeKey": "has_fixed_roles"
    },
    {
      "id": "q04",
      "hotkey": "4",
      "text": "Begrenzt es parallele Arbeit explizit über WIP-Limits (Work in Progress)?",
      "attributeKey": "uses_wip_limit"
    },
    {
      "id": "q05",
      "hotkey": "5",
      "text": "Ordnet es Spezifikationsschritten passende Teststufen zu (Unit/Integration/System/Abnahme)?",
      "attributeKey": "maps_spec_to_test_levels"
    },
    {
      "id": "q06",
      "hotkey": "6",
      "text": "Ist es ein Verbesserungs-Loop mit den Schritten Plan–Do–Check–Act?",
      "attributeKey": "continuous_improvement_loop"
    },
    {
      "id": "q07",
      "hotkey": "7",
      "text": "Ist es eine zertifizierbare QM-Norm für ein Qualitätsmanagementsystem?",
      "attributeKey": "qms_standard_certifiable"
    },
    {
      "id": "q08",
      "hotkey": "8",
      "text": "Steht es für Qualität als Unternehmenskultur/Haltung (alle sind verantwortlich)?",
      "attributeKey": "quality_culture_mindset"
    },
    {
      "id": "q09",
      "hotkey": "9",
      "text": "Hilft es dabei, Ziele als spezifisch, messbar, akzeptiert, realistisch und terminiert zu formulieren?",
      "attributeKey": "goal_definition_smart"
    },
    {
      "id": "q10",
      "hotkey": "10",
      "text": "Zerlegt es ein Projekt hierarchisch in Arbeitspakete (klarer Output pro Paket)?",
      "attributeKey": "work_breakdown_structure_psp"
    },
    {
      "id": "q11",
      "hotkey": "11",
      "text": "Schätzt es Aufwand über optimistisch/realistisch/pessimistisch (Bandbreite statt Bauchgefühl)?",
      "attributeKey": "effort_estimation_three_point"
    },
    {
      "id": "q12",
      "hotkey": "12",
      "text": "Dreht es sich um Abhängigkeiten und die längste Kette, die die Mindestdauer bestimmt (kritischer Pfad)?",
      "attributeKey": "schedule_planning_critical_path"
    },
    {
      "id": "q13",
      "hotkey": "13",
      "text": "Bewertet es Risiken als Eintrittswahrscheinlichkeit × Auswirkung und leitet Maßnahmen ab?",
      "attributeKey": "risk_management_probability_impact"
    },
    {
      "id": "q14",
      "hotkey": "14",
      "text": "Steuert es Änderungen formal (Bewertung + Freigabe), statt schleichend den Scope zu erweitern?",
      "attributeKey": "scope_change_control_formal"
    }
  ]
}
