  "questions": [
    {
      "id": "q01",
      "hotkey": "1",
      "text": "Ist es eine grafische Darstellung bzw. ein Diagramm?",
      "attributeKey": "is_visual_diagram"
    },
    {
      "id": "q02",
      "hotkey": "2",
      "text": "Beschreibt es vor allem, was ein System aus Nutzersicht können soll?",
      "attributeKey": "answers_what_requirements"
    },
    {
      "id": "q03",
      "hotkey": "3",
      "text": "Modelliert es Klassen mit Attributen, Methoden und Beziehungen?",
      "attributeKey": "models_classes_structure"
    },
    {
      "id": "q04",
      "hotkey": "4",
      "text": "Zeigt es einen Ablauf bzw. Workflow mit Entscheidungen/Verzweigungen?",
      "attributeKey": "models_workflow_logic"
    },
    {
      "id": "q05",
      "hotkey": "5",
      "text": "Zeigt es Nachrichten/Calls zwischen Komponenten in zeitlicher Reihenfolge?",
      "attributeKey": "shows_message_sequence"
    },
    {
      "id": "q06",
      "hotkey": "6",
      "text": "Beschreibt es Zustände und Übergänge eines Objekts (Lebensphasen)?",
      "attributeKey": "shows_states_transitions"
    },
    {
      "id": "q07",
      "hotkey": "7",
      "text": "Beschreibt es ein Datenmodell mit Entitäten, Beziehungen und PK/FK?",
      "attributeKey": "models_database_schema"
    },
    {
      "id": "q08",
      "hotkey": "8",
      "text": "Beschreibt es Logik/Algorithmus als Text ohne sprachspezifische Syntax?",
      "attributeKey": "describes_algorithm_textually"
    },
    {
      "id": "q09",
      "hotkey": "9",
      "text": "Nutzt es typische Flowchart-Symbole (Start/Ende, Prozess, Entscheidung, Ein-/Ausgabe)?",
      "attributeKey": "uses_flowchart_symbols"
    },
    {
      "id": "q10",
      "hotkey": "10",
      "text": "Ist es blockbasiert und kommt ohne Pfeile aus (verschachtelte Blöcke)?",
      "attributeKey": "uses_block_structure"
    },
    {
      "id": "q11",
      "hotkey": "11",
      "text": "Gehört es zu SQL-Abfragen wie SELECT/WHERE/ORDER BY/LIMIT?",
      "attributeKey": "sql_basic_select_where"
    },
    {
      "id": "q12",
      "hotkey": "12",
      "text": "Verknüpft es Tabellen über Schlüsselbezüge (JOIN)?",
      "attributeKey": "sql_joins_tables"
    },
    {
      "id": "q13",
      "hotkey": "13",
      "text": "Arbeitet es mit Aggregaten und Gruppierung (GROUP BY, COUNT/SUM/AVG, HAVING)?",
      "attributeKey": "sql_group_by_aggregation"
    },
    {
      "id": "q14",
      "hotkey": "14",
      "text": "Geht es um Konsistenz/Redundanz/Regeln im Datenbank-Design oder -Betrieb (z. B. Normalformen, ACID)?",
      "attributeKey": "focuses_on_db_consistency_rules"
    }
  ]
