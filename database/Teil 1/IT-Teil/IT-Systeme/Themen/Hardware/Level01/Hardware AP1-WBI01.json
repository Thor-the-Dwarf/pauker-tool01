  "questions": [
    {
      "id": "q01",
      "hotkey": "1",
      "text": "Bin ich das Rechenwerk, das Befehle ausführt (Kerne/Threads, Takt, Cache)?",
      "attributeKey": "is_cpu_compute"
    },
    {
      "id": "q02",
      "hotkey": "2",
      "text": "Speichere ich Daten nur flüchtig für laufende Programme (RAM)?",
      "attributeKey": "is_ram_volatile"
    },
    {
      "id": "q03",
      "hotkey": "3",
      "text": "Speichere ich Daten dauerhaft (Massenspeicher)?",
      "attributeKey": "is_persistent_storage"
    },
    {
      "id": "q04",
      "hotkey": "4",
      "text": "Bin ich die zentrale Platine, die Komponenten verbindet (Mainboard/Chipsatz)?",
      "attributeKey": "is_mainboard"
    },
    {
      "id": "q05",
      "hotkey": "5",
      "text": "Versorge ich alle Komponenten mit Strom (Netzteil)?",
      "attributeKey": "provides_power"
    },
    {
      "id": "q06",
      "hotkey": "6",
      "text": "Gebe ich Bild/Grafik an Monitor(e) aus (Grafikkarte/GPU)?",
      "attributeKey": "provides_graphics_output"
    },
    {
      "id": "q07",
      "hotkey": "7",
      "text": "Bin ich hauptsächlich zum Kühlen da (Kühler/Lüfter)?",
      "attributeKey": "is_cooling_component"
    },
    {
      "id": "q08",
      "hotkey": "8",
      "text": "Bin ich das Gehäuse, das alles aufnimmt und schützt?",
      "attributeKey": "is_enclosure"
    },
    {
      "id": "q09",
      "hotkey": "9",
      "text": "Stecke ich typischerweise in einem PCIe-Slot?",
      "attributeKey": "plugs_into_pcie_slot"
    },
    {
      "id": "q10",
      "hotkey": "10",
      "text": "Stecke ich typischerweise in einem DIMM-Slot?",
      "attributeKey": "plugs_into_dimm_slot"
    },
    {
      "id": "q11",
      "hotkey": "11",
      "text": "Werde ich typischerweise in einen CPU-Sockel eingesetzt?",
      "attributeKey": "plugs_into_cpu_socket"
    },
    {
      "id": "q12",
      "hotkey": "12",
      "text": "Bin ich über SATA angeschlossen (Datenkabel)?",
      "attributeKey": "connects_via_sata"
    },
    {
      "id": "q13",
      "hotkey": "13",
      "text": "Bin ich über M.2 (NVMe) angeschlossen?",
      "attributeKey": "connects_via_m2"
    },
    {
      "id": "q14",
      "hotkey": "14",
      "text": "Nutze ich Flash-Speicher (NAND)?",
      "attributeKey": "uses_nand_flash"
    },
    {
      "id": "q15",
      "hotkey": "15",
      "text": "Nutze ich magnetische Scheiben (Platter) zur Datenspeicherung?",
      "attributeKey": "uses_magnetic_platters"
    },
    {
      "id": "q16",
      "hotkey": "16",
      "text": "Gehöre ich zu Netzwerk-Hardware (LAN/WLAN/Switch/Router)?",
      "attributeKey": "is_network_hardware"
    },
    {
      "id": "q17",
      "hotkey": "17",
      "text": "Biete ich LAN/Ethernet (Kabel) an?",
      "attributeKey": "provides_wired_ethernet"
    },
    {
      "id": "q18",
      "hotkey": "18",
      "text": "Biete ich WLAN (Funk) an?",
      "attributeKey": "provides_wifi"
    },
    {
      "id": "q19",
      "hotkey": "19",
      "text": "Leite ich Daten im LAN weiter (Switching)?",
      "attributeKey": "performs_switching"
    },
    {
      "id": "q20",
